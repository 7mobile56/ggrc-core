<spinner toggle="isLoading" extra-css-class="{{spinnerCss}}" class="spinner-wrapper {{#if isLoadingInProgress}}active{{/if}}"></spinner>
{{#each items}}
  <object-list-item selected-item="selectedItem" index="{{@index}}" can-click="select">
    <content></content>
  </object-list-item>
{{/each}}

